{
    "name": "drip-search",
    "version": "0.1.1",
    "manifest_version": 2,
    "description": "Filter your search results by 'past year'",
    "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "lib/background/index.js"
        ]
    },
    "commands": {
        "pastYear": {
            "description": "Filter search results by 'past year'"
        },
        "twoYearsAgo": {
            "description": "Filter search results by '2 years ago'"
        }
    },
    "permissions": [
        "tabs",
        "http://www.google.com/",
        "https://www.google.com/",
        "http://www.google.co.jp/",
        "https://www.google.co.jp/"
    ]
}
